                    </gmd:organisationName>
                    <gmd:role>
                        <gmd:CI_RoleCode codeListValue="custodian"
                                         codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#CI_RoleCode">custodian</gmd:CI_RoleCode>
                    </gmd:role>
                </gmd:CI_ResponsibleParty>
            </gmd:pointOfContact>

            <!-- Maintenance frequency -->
            <!-- Mandatory -->
            <!-- e.g. probably "notplanned" is a good choice for these data -->
            <!-- Table 33 pag A-39-->

            <gmd:resourceMaintenance>
                <gmd:MD_MaintenanceInformation>
                    <!-- RSMFRQ [1] -->
                    <gmd:maintenanceAndUpdateFrequency>
                        <gmd:MD_MaintenanceFrequencyCode codeListValue=""
                                                         codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#MD_MaintenanceFrequencyCode"></gmd:MD_MaintenanceFrequencyCode>
                    </gmd:maintenanceAndUpdateFrequency>
                </gmd:MD_MaintenanceInformation>
            </gmd:resourceMaintenance>

            <!-- Graphic overview -->
            <!-- Optional -->
            <!-- GP:??? only one graphic overview???? What if there is no graphic overview, probably the placeholder should incorporate the xml tags as well-->
            <!-- GP:??? us eplaceholder here i want to set from the script thumbnail, plot, TSplot, vertical plot -->
            <!-- THUMB [0..1] -->

            <gmd:graphicOverview>
                <gmd:MD_BrowseGraphic>
                    <gmd:fileName>
                        <gco:CharacterString></gco:CharacterString>
                    </gmd:fileName>
                    <gmd:fileDescription>
                        <gco:CharacterString>thumbnail</gco:CharacterString>
                    </gmd:fileDescription>
                </gmd:MD_BrowseGraphic>
            </gmd:graphicOverview>


            <!-- Keywords. These are from fixed codelists -->
            <!-- eg: vector2D -->
            <!-- Table 23 Page A-30 -->

            <!-- DGITYP [0..1] -->
            <gmd:descriptiveKeywords>
                <gmd:MD_Keywords>
                    <gmd:keyword>
                        <ngmp:NGMP_GeospatialInformationTypeCode codeListValue=""
                                                                 codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#NGMP_GeospatialInformationTypeCode"></ngmp:NGMP_GeospatialInformationTypeCode>
                    </gmd:keyword>
                    <gmd:thesaurusName>
                        <gmd:CI_Citation>
                            <gmd:title>
                                <gco:CharacterString>NGMP_GeospatialInformationTypeCode</gco:CharacterString>
                            </gmd:title>
                            <gmd:date>
                                <gmd:CI_Date>
                                    <gmd:date>
                                        <gco:Date>2011-09-16</gco:Date>
                                    </gmd:date>
                                    <gmd:dateType>
                                        <gmd:CI_DateTypeCode codeListValue="creation"
                                                             codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#CI_DateTypeCode">creation</gmd:CI_DateTypeCode>
                                    </gmd:dateType>
                                </gmd:CI_Date>
                            </gmd:date>
                        </gmd:CI_Citation>
                    </gmd:thesaurusName>
                </gmd:MD_Keywords>
            </gmd:descriptiveKeywords>

            <!-- Keywords. These are from fixed codelists -->
            <!-- eg: georeferenced -->
            <!-- Table 22 Page A-30 -->

            <!-- RSGFLV [0..1] -->
            <gmd:descriptiveKeywords>
                <gmd:MD_Keywords>
                    <gmd:keyword>
                        <ngmp:NGMP_GeoreferencingLevelCode codeListValue=""
                                                           codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#NGMP_GeoreferencingLevelCode"></ngmp:NGMP_GeoreferencingLevelCode>
                    </gmd:keyword>
                    <gmd:thesaurusName>
                        <gmd:CI_Citation>
                            <gmd:title>
                                <gco:CharacterString>NGMP_GeoreferencingLevelCode</gco:CharacterString>
                            </gmd:title>
                            <gmd:date>
                                <gmd:CI_Date>
                                    <gmd:date>
                                        <gco:Date>2011-09-16</gco:Date>
                                    </gmd:date>
                                    <gmd:dateType>
                                        <gmd:CI_DateTypeCode codeListValue="creation"
                                                             codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#CI_DateTypeCode">creation</gmd:CI_DateTypeCode>
                                    </gmd:dateType>
                                </gmd:CI_Date>
                            </gmd:date>
                        </gmd:CI_Citation>
                    </gmd:thesaurusName>
                </gmd:MD_Keywords>
            </gmd:descriptiveKeywords>

            <!-- Keywords. These are from fixed codelists -->
            <!-- In our case, we can assume "digital" is always ok -->
            <!-- Table 24 Page A-31 -->

            <!-- RSPREF [0..1] -->
            <gmd:descriptiveKeywords>
                <gmd:MD_Keywords>
                    <gmd:keyword>
                        <ngmp:NGMP_RepresentationFormCode codeListValue="digital"
