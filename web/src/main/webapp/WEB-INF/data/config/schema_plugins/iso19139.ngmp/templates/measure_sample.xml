<?xml version="1.0" encoding="UTF-8"?>
<gmd:MD_Metadata xmlns:ngmp="urn:int:nato:geometoc:geo:metadata:ngmp:1.0"
                 xmlns:gmd="http://www.isotc211.org/2005/gmd"
                 xmlns:gml="http://www.opengis.net/gml"
                 xmlns:gco="http://www.isotc211.org/2005/gco"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns:xlink="http://www.w3.org/1999/xlink"
                 xsi:schemaLocation="
                        http://www.isotc211.org/2005/gmd            http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/gmd/gmd.xsd
                        http://www.opengis.net/gml                  http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19136_Schemas/gml.xsd
                        http://www.isotc211.org/2005/gco            http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/gco/gco.xsd
                        urn:int:nato:geometoc:geo:metadata:ngmp:1.0 https://nmrr.nc3a.nato.int/filerepo/GeoMetOc/metadata/ngmp/1.0/ngmp.xsd">

    <!-- NOTE about GML URI:
         STANAG defines the GML URI to be http://www.opengis.net/gml/3.2, but most of GN internals work with http://www.opengis.net/gml
         Internally NGMP metadata will have the /gml URI, as in this template.
         Anytime NGMP documents will be exported,  /gml URI will be converted into /gml/3.2
    -->

    <!-- Metadata uuid, mandatory -->
    <!-- Format is  {HHHHHHHH-HHHH-HHHH-HHHH-HHHHHHHHHHHH} braces included, here H is an hex digit-->
    <!-- MDSID [1] -->
    <gmd:fileIdentifier>
        <gco:CharacterString></gco:CharacterString>
    </gmd:fileIdentifier>

    <!-- Metadata language, mandatory -->
    <!-- We can keep this fixed as "eng" -->
    <!-- MDLANG [1] -->
    <gmd:language>
        <gmd:LanguageCode codeListValue="eng"
                          codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#LanguageCode">eng</gmd:LanguageCode>
    </gmd:language>

    <!-- Metadata encoding, mandatory -->
    <!-- We can keep this fixed as "utf8" -->
    <!-- MDCSET [1] -->
    <gmd:characterSet>
        <gmd:MD_CharacterSetCode codeListValue="utf8"
                                 codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#MD_CharacterSetCode">utf8</gmd:MD_CharacterSetCode>
    </gmd:characterSet>

    <!-- Resource type, mandatory -->
    <!-- We may keep this fixed as "dataset" -->
    <!-- RSTYPE [1]: Only the first instance is in the scope of NGMP -->
    <gmd:hierarchyLevel>
        <gmd:MD_ScopeCode codeListValue="dataset"
                          codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#MD_ScopeCode">dataset</gmd:MD_ScopeCode>
    </gmd:hierarchyLevel>

    <!-- Point of contact for the metadata. -->
    <!-- At least one is required. -->
    <!-- Other responsible parties may be added with different roles (author, processor, owner, ...): see table 39, page A-42 -->
    <!-- We may keep this first element as fixed, and add other when/if required. It's a complex element, so no placeholder here.-->
    <!-- MDPOC [1..*] -->
    <gmd:contact>
        <gmd:CI_ResponsibleParty>
            <!-- MDPOC.PtOrgName -->
            <gmd:organisationName>
                <gco:CharacterString></gco:CharacterString>
            </gmd:organisationName>
            <gmd:contactInfo>
                <gmd:CI_Contact>
                    <gmd:phone>
                        <gmd:CI_Telephone>
                            <gmd:voice>
                                <gco:CharacterString></gco:CharacterString>
                            </gmd:voice>
                            <gmd:facsimile>
                                <gco:CharacterString></gco:CharacterString>
                            </gmd:facsimile>
                        </gmd:CI_Telephone>
                    </gmd:phone>
                    <gmd:address>
                        <gmd:CI_Address>
                            <gmd:deliveryPoint>
                                <gco:CharacterString></gco:CharacterString>
                            </gmd:deliveryPoint>
                            <gmd:city>
                                <gco:CharacterString></gco:CharacterString>
                            </gmd:city>
                            <gmd:administrativeArea gco:nilReason="missing">
                                <gco:CharacterString/>
                            </gmd:administrativeArea>
                            <gmd:postalCode>
                                <gco:CharacterString></gco:CharacterString>
                            </gmd:postalCode>
                            <gmd:country>
                                <gco:CharacterString></gco:CharacterString>
                            </gmd:country>
                            <gmd:electronicMailAddress>
                                <gco:CharacterString></gco:CharacterString>
                            </gmd:electronicMailAddress>
                        </gmd:CI_Address>
                    </gmd:address>
                    <gmd:onlineResource>
                        <gmd:CI_OnlineResource>
                            <gmd:linkage>
                                <gmd:URL></gmd:URL>
                            </gmd:linkage>
                            <gmd:protocol>
                                <gco:CharacterString>WWW:LINK-1.0-http--link</gco:CharacterString>
                            </gmd:protocol>
                            <gmd:name gco:nilReason="missing">
                                <gco:CharacterString/>
                            </gmd:name>
                            <gmd:description gco:nilReason="missing">
                                <gco:CharacterString/>
                            </gmd:description>
                        </gmd:CI_OnlineResource>
                    </gmd:onlineResource>
                </gmd:CI_Contact>
            </gmd:contactInfo>

            <gmd:role>
                <!-- Table 39, page A-42 -->
                <gmd:CI_RoleCode codeListValue="pointOfContact"
                                 codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#CI_RoleCode">pointOfContact</gmd:CI_RoleCode>
            </gmd:role>

        </gmd:CI_ResponsibleParty>
    </gmd:contact>

    <!-- Date of creation or last update of the metadata. -->
    <!-- Mandatory -->
    <!-- MDDATE [1] -->
    <gmd:dateStamp>
        <gco:Date>__MDDATE__</gco:Date>
    </gmd:dateStamp>

    <!-- Info about the metadata stadard. -->
    <!-- Mandatory, fixed value -->
    <!-- MDSTD [1]: There shall be an instance of gmd:metadataStandardName having the 'STANAG 2586' value and an
    instance of having the value "Edition 1" -->
    <!-- MDSTD.CiTitle [1] -->
    <gmd:metadataStandardName>
        <gco:CharacterString>STANAG 2586</gco:CharacterString>
    </gmd:metadataStandardName>
    <!-- Info about the metadata stadard. -->
    <!-- Mandatory, fixed value -->
    <!-- MDSTD.CiEdition [1] -->
    <gmd:metadataStandardVersion>
        <gco:CharacterString>Edition 1</gco:CharacterString>
    </gmd:metadataStandardVersion>

    <!-- Spatial or temporal RS used in the resource -->
    <!-- e.g. "EPSG:4326" -->
    <!-- RSRSYS [1..*] -->
    <gmd:referenceSystemInfo>
        <gmd:MD_ReferenceSystem>
            <gmd:referenceSystemIdentifier>
                <gmd:RS_Identifier>
                    <gmd:code>
                        <gco:CharacterString></gco:CharacterString>
                    </gmd:code>
                </gmd:RS_Identifier>
            </gmd:referenceSystemIdentifier>
        </gmd:MD_ReferenceSystem>
    </gmd:referenceSystemInfo>

    <gmd:identificationInfo>
        <gmd:MD_DataIdentification>
            <gmd:citation>
                <gmd:CI_Citation>
                    <!-- Unique name by which the resource is known -->
                    <!-- Mandatory -->
                    <!-- RSTITLE [1] -->
                    <gmd:title>
                        <gco:CharacterString></gco:CharacterString>
                    </gmd:title>
                    <!-- Alternate title -->
                    <!-- Optional -->
                    <!-- RSALT [0..1] -->
                    <!--                    <gmd:alternateTitle>
                                            <gco:CharacterString>REP11 REP11A CTD</gco:CharacterString>
                                        </gmd:alternateTitle>
                    -->
                    <!-- Resource ==dato, creation date -->
                    <!-- Mandatory -->
                    <!-- e.g. 2011-01-08 -->
                    <!-- RSCDAT [1] -->
                    <!-- GP:??? differza con MDDATE, quando e' stato creato il dato -->
                    <gmd:date>
                        <gmd:CI_Date>
                            <gmd:date>
                                <gco:Date></gco:Date>
                            </gmd:date>
                            <gmd:dateType>
                                <gmd:CI_DateTypeCode codeListValue="creation"
                                                     codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#CI_DateTypeCode">creation</gmd:CI_DateTypeCode>
                            </gmd:dateType>
                        </gmd:CI_Date>
                    </gmd:date>

                    <!-- If RSID is set -->
                    <!--                    <gmd:identifier>
                        <gmd:MD_Identifier>
                             RSID [0..1]
                            <gmd:code>
                                <gco:CharacterString>{fd37cf40-d927-11e0-9572-0800200c9a66}</gco:CharacterString>
                            </gmd:code>
                        </gmd:MD_Identifier>
                    </gmd:identifier>-->
                    <!-- RSSERI [0..1]: Mandated if RSSHNA is set -->
                    <!--                    <gmd:series>
                        <gmd:CI_Series>
                            <gmd:name>
                                <gco:CharacterString>AOR MISC 020</gco:CharacterString>
                            </gmd:name>
                        </gmd:CI_Series>
                    </gmd:series>-->
                </gmd:CI_Citation>
            </gmd:citation>

            <!-- Description of the resource -->
            <!-- Mandatory -->
            <!-- RSABSTR [1] -->
            <!-- GP:??? chiedere al data keeper: free text -->
            <gmd:abstract>
                <gco:CharacterString></gco:CharacterString>
            </gmd:abstract>

            <!-- Resource status -->
            <!-- Optional -->
            <!-- Refer to table 32 page A-38 -->
            <!-- RSSTAT [0..1] -->
            <!-- GP:??? chidere ale -->
            <gmd:status>
                <gmd:MD_ProgressCode codeListValue=""
                                     codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#MD_ProgressCode"></gmd:MD_ProgressCode>
            </gmd:status>

            <!-- Resource originator -->
            <!-- All data are from CMRE, so no need to put a placeholder here -->

            <!-- If RSOORG[0..1] and/or RSONAT[0..1] -->
            <gmd:pointOfContact>
                <gmd:CI_ResponsibleParty>
                    <!-- RSOORG if set, else RSONAT -->
                    <gmd:organisationName>
                        <gco:CharacterString></gco:CharacterString>
                    </gmd:organisationName>
                    <!-- if RSONAT is set -->
                    <!--
                                         <gmd:contactInfo>
                                            <gmd:CI_Contact>
                                                <gmd:address>
