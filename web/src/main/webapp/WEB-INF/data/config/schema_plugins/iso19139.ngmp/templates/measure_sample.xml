                                                            <gco:CharacterString></gco:CharacterString>
                                                        </gmd:country>
                                                    </gmd:CI_Address>
                                                </gmd:address>
                                            </gmd:CI_Contact>
                                        </gmd:contactInfo>
                    -->
                    <gmd:role>
                        <gmd:CI_RoleCode codeListValue="originator"
                                         codeList="http://eden.ign.fr/xsd/ngmp/20110916/resources/codelist/ngmpCodelists.xml#CI_RoleCode">originator</gmd:CI_RoleCode>
                    </gmd:role>
                </gmd:CI_ResponsibleParty>
            </gmd:pointOfContact>

            <!-- Resource custodian -->
            <!-- We may have the fixed value "nurc" here -->
            <!-- Custodian body list is in table 40 page A-43 -->

            <!-- For each RSCUST [0..*] -->
            <!-- GP:??? ask ale for nurc or cmre, fixed in the template -->
            <gmd:pointOfContact>
                <gmd:CI_ResponsibleParty>
